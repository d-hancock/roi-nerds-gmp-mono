{
  "name": "roi-nerds-gmp-mono",
  "packageManager": "yarn@3.2.0",
  "private": true,
  "scripts": {
    "typesync:all": "yarn workspaces foreach run preinstall",
    "clean:all": "yarn workspaces foreach run clean",
    "preinstall": "typesync || :",
    "dev:roi-core": "yarn workspace @roi/gatsby-starter-roi-core develop",
    "clean:roi-core": "yarn workspace @roi/gatsby-starter-roi-core clean",
    "dev:roi-site": "yarn workspace @roi/roi-nerds-website develop",
    "clean:roi-site": "yarn workspace @roi/roi-nerds-website clean",
    "dev:comp-lib": "yarn workspace @roi/roi-comp-lib develop",
    "storybook:comp-lib": "yarn workspace @roi/roi-comp-lib storybook",
    "clean:comp-lib": "yarn workspace @roi/roi-comp-lib clean"
  },
  "workspaces": [
    "packages/component-libraries/*",
    "packages/sites/*",
    "packages/starters/*",
    "packages/themes/*",
    "packages/plugins/*"
  ],
  "dependencies": {
    "gatsby": "^4.13.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
