{
  "name": "roi-nerds-gmp-mono",
  "packageManager": "yarn@3.2.0",
  "private": true,
  "scripts": {
    "typesync:roi-core": "yarn workspaces foreach run preinstall",
    "preinstall": "typesync || :",
    "dev:roi-core": "yarn workspace @roi/gatsby-starter-roi-core develop",
    "clean:roi-core": "yarn workspace @roi/gatsby-starter-roi-core clean",
    "dev:comp-lib": "yarn workspace @roi/roi-comp-lib develop",
    "storybook:comp-lib": "yarn workspace @roi/roi-comp-lib storybook",
    "clean:comp-lib": "yarn workspace @roi/roi-comp-lib clean"
  },
  "workspaces": [
    "packages/component-libraries/*",
    "packages/sites/*",
    "packages/starters/*",
    "packages/themes/*"
  ],
  "dependencies": {
    "gatsby": "^4.13.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@types/react": "^17.0.44",
    "@types/react-dom": "^17.0.16",
    "typescript": "^4.6.3",
    "typesync": "^0.9.1"
  }
}
